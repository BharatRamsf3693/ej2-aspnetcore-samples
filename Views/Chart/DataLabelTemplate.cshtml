@using Syncfusion.EJ2
@section ControlsSection{
    <div class="control-section" align="center">
        <ejs-chart id="container" textRender="textRender" load="window.onChartLoad" title="Population of India Statistics" subTitle="(2010 - 2016)">
            <e-chart-chartarea>
                <e-chartarea-border width="0"></e-chartarea-border>
            </e-chart-chartarea>
            <e-chart-subtitlestyle textAlignment="Far"></e-chart-subtitlestyle>
            <e-chart-legendsettings visible="true">
            </e-chart-legendsettings>
            <e-chart-primaryxaxis minimum=2010 maximum=2016
                                  interval=1 edgeLabelPlacement="Shift">
                <e-majorgridlines width="0"></e-majorgridlines>
                <e-minorgridlines width="0"></e-minorgridlines>
                <e-linestyle width="1" color="#eaeaea"></e-linestyle>
                <e-labelstyle color="#606060" fontFamily="Roboto" size="14px" fontStyle="medium"></e-labelstyle>
            </e-chart-primaryxaxis>
            <e-chart-primaryyaxis minimum=900 maximum=1300 labelFormat="{value}M" interval=80>
                <e-linestyle width="1" color="#eaeaea"></e-linestyle>
                <e-labelstyle color="#606060" fontFamily="Roboto" size="14px" fontStyle="medium"></e-labelstyle>
                <e-majorgridlines width="1" color="#eaeaea"></e-majorgridlines>
            </e-chart-primaryyaxis>
            <e-series-collection>
                <e-series dataSource="ViewBag.dataSource" xName="x" yName="y" name="Male" width="2" opacity="1" type="@Syncfusion.EJ2.Charts.ChartSeriesType.Line">
                    <e-series-marker visible="true" shape="Circle">
                        <e-series-datalabel visible="true" position="Top" template="#Male-Material">
                            <e-datalabelsettings-margin right="30"></e-datalabelsettings-margin>
                        </e-series-datalabel>
                    </e-series-marker>
                </e-series>
                <e-series dataSource="ViewBag.dataSource1" xName="x" yName="y" name="Female" width="2" opacity="1" type="@Syncfusion.EJ2.Charts.ChartSeriesType.Line">
                    <e-series-marker visible="true" shape="Rectangle">
                        <e-series-datalabel visible="true" position="Bottom" template="#Female-Material">
                            <e-datalabelsettings-margin right="15"></e-datalabelsettings-margin>
                        </e-series-datalabel>
                    </e-series-marker>
                </e-series>
            </e-series-collection>
        </ejs-chart>
    </div>
    @*custom code start*@
    <div style="float: right; margin-right: 10px;">
    </div>
    <script>
        var theme;
        window.onChartLoad = function (args) {
            var selectedTheme = location.hash.split('/')[1];
            selectedTheme = selectedTheme ? selectedTheme : 'Material';
            args.chart.theme = (selectedTheme.charAt(0).toUpperCase() + selectedTheme.slice(1).replace(/-dark/i, 'Dark').replace(/contrast/i, 'Contrast'));
            args.chart.width = ej.base.Browser.isDevice ? "100%" : "80%";
            theme = args.chart.theme;
        };

        textRender = function (args) {
            args.template = '#' + args.series.name + '-' + theme;
        }
    </script>
    @*custom code end*@
